{
  "name": "translator-v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check:localhost": "node scripts/check-localhost.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:visual:update": "UPDATE_SNAPSHOTS=true playwright test --config=playwright.visual.config.ts --update-snapshots",
    "test:visual:debug": "playwright test --config=playwright.visual.config.ts --debug",
    "test:visual:report": "playwright show-report test-results/visual-regression-report",
    "ui:validate": "npm run test:visual -- --grep='UI Contract: Validation'",
    "ui:baseline": "npm run test:visual:update",
    "setup:git-hooks": "node scripts/setup-git-hooks.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.1",
    "openai": "^5.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.9.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^17.1.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.7.1",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "unocss": "^66.3.3",
    "vite": "^5.4.1",
    "vitest": "^3.2.4"
  }
}
